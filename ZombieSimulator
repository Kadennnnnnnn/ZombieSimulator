--Properties:

ZombieSimulator.Name = "ZombieSimulator"
ZombieSimulator.Parent = game.CoreGui

Main.Name = "Main"
Main.Parent = ZombieSimulator
Main.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Main.BorderColor3 = Color3.fromRGB(30, 30, 30)
Main.Position = UDim2.new(0.22924529, 0, 0.119180635, 0)
Main.Size = UDim2.new(0, 620, 0, 353)
Main.Draggable = true
Main.Active = true
Main.Selectable = true

Scripts.Name = "Scripts"
Scripts.Parent = Main
Scripts.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
Scripts.BorderColor3 = Color3.fromRGB(60, 60, 60)
Scripts.Position = UDim2.new(0.0290322583, 0, 0.156644911, 0)
Scripts.Size = UDim2.new(0, 586, 0, 284)

AutoEat.Name = "AutoEat"
AutoEat.Parent = Scripts
AutoEat.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
AutoEat.BorderColor3 = Color3.fromRGB(85, 255, 0)
AutoEat.BorderSizePixel = 0
AutoEat.Position = UDim2.new(0.0252768062, 0, 0.0479664207, 0)
AutoEat.Size = UDim2.new(0, 156, 0, 49)
AutoEat.ZIndex = 2
AutoEat.Font = Enum.Font.Creepster
AutoEat.Text = "AUTO EAT"
AutoEat.TextColor3 = Color3.fromRGB(0, 0, 0)
AutoEat.TextScaled = true
AutoEat.TextSize = 14.000
AutoEat.TextWrapped = true
AutoEat.MouseButton1Down:connect(function()
while true do
	wait("0")

game:GetService("ReplicatedStorage").Events.Eat:FireServer()
end
end)

