-- Instances:

local ZombieSimulator = Instance.new("ScreenGui")
local Main = Instance.new("Frame")
local Scripts = Instance.new("Frame")
local AutoEat = Instance.new("TextButton")
local AutoSell = Instance.new("TextButton")
local BuyAllBrains = Instance.new("TextButton")
